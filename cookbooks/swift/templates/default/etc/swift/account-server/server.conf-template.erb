[DEFAULT]
disable_fallocate = true
workers = 1

[pipeline:main]
pipeline = backend_ratelimit recon account-server

[app:account-server]
use = egg:swift#account
<% if ! @replication_server -%>
replication_server = false
<% end -%>

[filter:recon]
use = egg:swift#recon

[filter:backend_ratelimit]
use = egg:swift#backend_ratelimit
config_reload_interval = 60

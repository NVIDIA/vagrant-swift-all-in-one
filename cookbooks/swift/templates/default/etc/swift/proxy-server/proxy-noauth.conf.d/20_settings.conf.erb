[DEFAULT]
bind_port = 8090
<% if @statsd_exporter -%>
log_statsd_host = localhost
log_statsd_port = 9125
<% end -%>

[pipeline:main]
pipeline = catch_errors gatekeeper healthcheck <% if @tracing -%>trace <% end -%> proxy-logging cache list-endpoints bulk tempurl slo dlo versioned_writes symlink proxy-logging proxy-server

[filter:gatekeeper]
use = egg:swift#gatekeeper
shunt_inbound_x_timestamp = false
allow_reserved_names_header = true

[filter:list-endpoints]
use = egg:swift#list_endpoints
default_response_version = v2
